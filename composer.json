{
    "name": "ebrana/elasticsearch-bundle",
    "license": "proprietary",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Elasticsearch\\": "src/Elasticsearch"
        }
    },
    "require": {
        "php": ">=8.3.0",
        "elasticsearch/elasticsearch": "~v8.12.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpstan/phpstan": "~1.10.47",
        "phpstan/phpstan-php-parser": "~1.1",
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
